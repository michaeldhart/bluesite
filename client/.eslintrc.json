{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 13,
    "sourceType": "module"
  },
  "plugins": ["react", "@typescript-eslint"],
  "rules": {
    "prefer-arrow-callback": "error",
    "quotes": ["error", "single"]
  }
}
